<?xml version="1.0" encoding="UTF-8" ?>
<resources>
    <attr name="fromDegrees" format="float|reference"/>
    <attr name="toDegrees" format="float|reference"/>
    <attr name="childSize" format="dimension|reference"/>


    <declare-styleable name="ArcLayout">
        <attr name="fromDegrees"/>
        <attr name="toDegrees"/>
        <attr name="childSize"/>
    </declare-styleable>

    <declare-styleable name="TagCloudView">
        <attr name="autoScrollMode">
            <enum name="disable" value="0"/>
            <enum name="decelerate" value="1"/>
            <enum name="uniform" value="2"/>
        </attr>

        <attr name="radiusPercent" format="float"/>
        <attr name="scrollSpeed" format="float"/>
        <attr name="lightColor" format="color"/>
        <attr name="darkColor" format="color"/>
    </declare-styleable>

</resources>